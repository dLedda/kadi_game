{
    "defaultSeverity": "error",
    "extends": [
        "tslint-react"
    ],
    "jsRules": {
    },
    "rules": {
        "jsx-no-multiline-js": false,
        "member-access": false,
        "prefer-for-of": true,
        "prefer-const": true,
        "prefer-readonly": true,
        "typedef": [
            true,
            "call-signature",
            "property-declaration"
        ],
        "ordered-imports": false,
        "quotemark": false,
        "no-console": false,
        "jsx-no-lambda": false
    },
    "rulesDirectory": [
    ],
    "linterOptions": {
        "exclude": [
            "dist/**/*.js",
            "config/**/*.js",
            "node_modules/**/*.ts"
        ]
    }
}